<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagTextView">
        <attr name="ttv_tagSizeRatio" format="float|reference" />
        <attr name="ttv_tagStartRatio" format="float|reference" />
        <attr name="ttv_tagColor" format="color|reference" />
        <attr name="ttv_tagPosition" format="enum">
            <enum name="topStart" value="0" />
            <enum name="topEnd" value="1" />
            <enum name="bottomEnd" value="2" />
            <enum name="bottomStart" value="3" />
        </attr>
        <attr name="ttv_textRatio" format="float|reference" />
        <attr name="ttv_textRotate" format="float|reference" />
        <attr name="ttv_text" format="string|reference" />
        <attr name="ttv_textColor" format="color|reference" />
    </declare-styleable>
</resources>